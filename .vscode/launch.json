        {"name":"Python Debugger: Current File","type":"debugpy","request":"launch","program":"${file}","console":"integratedTerminal","env":{"PYTHONPYCACHEPREFIX":"${workspaceFolder}/.pycache"}},
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "demo.main:app",
                "--reload"
            ],
            "jinja": true,
            "envFile": "${workspaceFolder}/.env.local",
            "env": {
                "PYTHONPYCACHEPREFIX": "${workspaceFolder}/.pycache"
            }
        }
    ]
